class Solution {
    public int maximum69Number (int num) {
       char[] charecters = String.valueOf(num).toCharArray();
       for(int i = 0; i < charecters.length;i++){
            if(charecters[i] == '6'){
                charecters[i] = '9';
                break;
            }
       }
       return Integer.parseInt(new String(charecters));
    }
}
