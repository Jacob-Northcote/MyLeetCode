class Solution {
    public int numberOfBeams(String[] bank) {
        int curr = 0;
        int past = 0;
        int beams =0;
        for(int i = 0; i < bank.length; i++){//collum
            for (int j = 0; j < bank[i].length(); j++){//row
                if(bank[i].charAt(j) == '1'){
                    curr += 1;
                }
            }
            if (curr > 0) {
                beams += past * curr;
                past = curr;
            }
            curr = 0;
        }
        return beams;
    }
}
