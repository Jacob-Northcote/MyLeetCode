class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        Arrays.sort(nums);
        List<Integer> ret = new ArrayList<>();
        for(int i = 1; i < nums.length; i++){
            if(nums[i] == nums[i-1]){
                ret.add(nums[i]);
            }
        }
         return new int[]{ret.get(0), ret.get(1)};
    }
}
