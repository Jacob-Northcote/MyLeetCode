class Solution {
    public List<String> generateParenthesis(int n) {
        ArrayList<String> ret = new ArrayList<>();
        backTracking(ret,"",0,0,n);
        return ret;
    }
    public void backTracking(ArrayList<String> ret,String curr, int open, int close, int max){
            if(curr.length() == max * 2){
                ret.add(curr);
                return;
            }
            if(open < max){backTracking(ret, curr + "(", open + 1, close, max);}
            if(open > close){backTracking(ret, curr + ")", open, close + 1, max);}
        }
}
